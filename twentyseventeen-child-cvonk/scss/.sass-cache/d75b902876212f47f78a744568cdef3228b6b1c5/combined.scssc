/*--------------------------------------------------------------
5.0 Typography
--------------------------------------------------------------*/
@import url('https://fonts.googleapis.com/css?family=Montserrat|Roboto');
body,
button,
input,
select,
textarea {
  color: #333;
  font-family: "Segoe UI", "Roboto", "Libre Franklin", "Helvetica Neue", helvetica, arial, sans-serif;
  font-size: 15px;
  font-size: 0.9375rem;
  font-weight: 400;
  line-height: 1.66; }

h1,
h2,
h3,
h4,
h5,
h6 {
  clear: both;
  line-height: 1.4;
  font-family: 'Montserrat', sans-serif;
  color: #993300; }

h1 {
  font-size: 2.0rem;
  font-weight: 600;
  text-align: center;
  padding: 1.25em 0 0;
  margin: 0 0 0.75em; }

h2,
.home.blog .entry-title {
  font-size: 20px;
  font-size: 1.75rem;
  font-weight: 500;
  padding: 1.15em 0 0;
  margin: 0 0 0.625em; }

h3 {
  font-size: 18px;
  font-size: 1.5rem;
  font-weight: 450;
  padding: 1em 0 0;
  margin: 0 0 0.5em; }

h4 {
  font-size: 16px;
  font-size: 1.25rem;
  font-weight: 400;
  padding: .5em 0 0;
  margin: 0 0 0.3em; }

h5 {
  font-size: 13px;
  font-size: 1rem;
  font-weight: 400;
  letter-spacing: 0.15em;
  text-transform: none;
  padding: .25em 0 0;
  margin: 0 0 0.2em; }

h6 {
  font-size: 11px;
  font-size: 1rem;
  font-weight: 400;
  font-style: italic;
  padding: .5em 0 0;
  margin: 0 0 0.1em; }

p {
  margin: 0 0 1.5em;
  padding: 0; }

/*--------------------------------------------------------------
8.0 Lists
--------------------------------------------------------------*/
.entry-content ul > li,
.entry-content ol > li {
  margin: .3em 0 .3em 2em; }

/*--------------------------------------------------------------
9.0 Tables
--------------------------------------------------------------*/
table {
  width: 0; }

div.post table caption {
  width: auto;
  margin: 0 auto;
  background: white;
  font-size: 0.9em;
  line-height: 13px;
  padding: 2px 4px 5px;
  border: none;
  color: #999999 !important; }

/*--------------------------------------------------------------
10.0 Links
--------------------------------------------------------------*/
article.post a {
  border-bottom: 2px solid #b4e6fa;
  color: inherit;
  -webkit-transition: background 0.15s cubic-bezier(0.33, 0.66, 0.66, 1);
  transition: background 0.15s cubic-bezier(0.33, 0.66, 0.66, 1); }

article.post a:hover,
article.post a:active {
  outline: 0;
  color: inherit;
  background-color: rgba(180, 230, 250, 0.35); }

article.post a.hideAnchor:hover,
article.post a:active > img {
  background-color: white; }

article.post a.hideAnchor,
article.post a[href$=jpg],
article.post a[href$=jpeg],
article.post a[href$=svg],
article.post a[href$=png],
article.post a[href$=gif] {
  border-bottom: 0 !important;
  -webkit-transition-property: none !important;
  transition-property: none !important;
  box-shadow: none;
  transition: none;
  text-decoration: none; }

.entry-content a,
.entry-content a,
.entry-title a,
.entry-meta a,
.page-links a,
.page-links a .page-number,
.site-footer .widget-area ul li a,
.site-footer .widget-area ul li a {
  box-shadow: none;
  transition: none;
  text-decoration: none; }

.entry-content a:focus,
.entry-content a:hover,
.entry-summary a:focus,
.entry-summary a:hover,
.page-links a:focus .page-number,
.page-links a:hover .page-number,
.entry-footer a:focus,
.entry-footer a:hover,
.entry-footer .cat-links a:focus,
.entry-footer .cat-links a:hover,
.entry-footer .tags-links a:focus,
.entry-footer .tags-links a:hover {
  box-shadow: none;
  transition: none;
  text-decoration: none; }

/*.entry-content */
a > img {
  box-shadow: none;
  transition: none;
  text-decoration: none; }

/* remove link styling from starbox */
.abh_tab_content > .abh_tab > .abh_image > a {
  border: none; }

/*--------------------------------------------------------------
13.0 Layout
--------------------------------------------------------------*/
.wrap {
  max-width: 3000px; }

div.single-featured-image-header > img {
  max-height: 200px;
  width: auto; }

/*--------------------------------------------------------------
13.1 Header
