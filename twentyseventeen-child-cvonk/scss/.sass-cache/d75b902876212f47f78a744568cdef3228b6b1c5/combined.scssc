  color: #666666;
  caption-side: bottom;
  background-color: #f8f8f8;
  margin-top: .8em;
  margin-bottom: .8em; }

/*
 * tables
 */
.table-align-outside {
  float: right;
  position: relative;
  left: -50%;
  text-align: left; }

.table-align-inside {
  position: relative;
  left: 50%; }

.table-alignbottom-outside {
  position: relative; }

.table-alignbottom-inside {
  position: absolute;
  bottom: 0; }

table tr {
  border-bottom: none; }

.mathtbl > tbody > tr > td {
  border-collapse: collapse;
  border: 1px solid black;
  text-align: left; }

.mathtbl > tbody > tr > th {
  border-collapse: collapse;
  border: 1px solid black;
  background-color: #f5f5ff;
  color: black;
  text-align: center; }

.cvonktblsml > tbody > tr > td {
  text-align: center !important;
  border-collapse: collapse;
  padding: 0px; }

.cvonktblsml > tbody > tr > th {
  border-collapse: collapse;
  background-color: white;
  color: black;
  text-align: center; }

.cvonktblsml {
  font-size: 0.8em;
  margin: auto;
  border-spacing: 0px; }

.cvonktblsml > tbody > tr > th {
  border-bottom: 1px solid #ddd; }

.cvonktblsml > tbody > tr > th.verticalBorderRight,
.cvonktblsml > tbody > tr > td.verticalBorderRight {
  border-right: 1px solid #ddd; }

/*
 * table borders
 */
.cvonktbl th.verticalBorderRight,
.cvonktbl td.verticalBorderRight,
.cvonktblsml th.verticalBorderRight,
.cvonktblsml td.verticalBorderRight {
  border-right: 1px solid #ddd; }

.cvonktbl tr:nth-child(1),
.cvonktblsml tr:nth-child(1) {
  border-bottom: 1px solid #ddd; }

button.circuit-animate:hover {
  box-shadow: 2px 8px 4px -6px rgba(0, 0, 0, 0.3); }

/*
 * scale fonts proportional to container size (such as tables or LaTex) 
 */
.scale-font {
  font-size: 1.5vw; }

@media screen and (min-width: 1000px) {
  .scale-font {
    font-size: 1rem; } }

/* option not to print 
 */
@media print {
  .no-print,
  .no-print * {
    display: none !important; } }
